<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.pregnant.health"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activity.WelcomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ActivityTheme.Full"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ui.activity.RoutineSurveyActivity"/>
        <activity android:name=".ui.activity.LoginActivity"/>
        <activity android:name=".ui.activity.MealSurveyActivity"/>
        <activity android:name=".ui.activity.CategoryListActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".ui.activity.RoutineSurveyOneActivity"/>
        <activity android:name=".ui.activity.RoutineSurveyTwoActivity"/>
        <activity android:name=".ui.activity.RoutineSurveyThreeActivity"/>
        <activity android:name=".ui.activity.RoutineSurveyFourActivity"/>
        <activity android:name=".ui.activity.RoutineSurveyFiveActivity"/>
        <activity android:name=".ui.activity.ChoicePicActivity"/>
        <activity android:name=".ui.activity.RegisterActivity"/>
        <activity android:name=".ui.activity.MealRecordActivity"/>
        <activity android:name=".ui.activity.MealDetailActivity"/>
        <activity android:name=".ui.activity.SportSurveyActivity"/>
        <activity android:name=".ui.activity.OccupationListActivity"/>
        <activity android:name=".ui.activity.SportSurveyChildActivity"/>
        <activity android:name=".ui.activity.PersonalInfoActivity"/>
        <activity android:name=".ui.activity.SystemAnalysisActivity"/>
        <activity android:name=".ui.activity.AnalysisResultActivity"/>
        <activity android:name=".ui.activity.GuideLinesActivity"/>
        <activity
            android:name="com.artifex.mupdflib.MuPDFActivity"
            android:theme="@style/AppReaderTheme"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.artifex.mupdflib.PrintDialogActivity"
            android:theme="@style/AppReaderTheme"
            android:label="@string/print">
        </activity>
        <activity
            android:name="com.artifex.mupdflib.PDFPreviewGridActivity"
            android:theme="@style/AppReaderTheme"
            android:label="preview label">
        </activity>
    </application>

</manifest>
